{
  "name": "sensor",
  "plural": "sensors",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "key": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "pattern": "/OK|NOK/"
    },
    "createdAt": {
      "type": "date"
    },
    "modifiedAt": {
      "type": "date"
    },
    "checkedAt": {
      "type": "date"
    },
    "frequency": {
      "type": "number",
      "required": true
    },
    "comments": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "logs": {
      "type": "hasMany",
      "model": "event",
      "foreignKey": "sensorId"
    }
  },
  "acls": [],
  "methods": []
}
