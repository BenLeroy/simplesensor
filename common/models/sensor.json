{
  "name": "sensor",
  "plural": "sensors",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "key": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date"
    },
    "modifiedAt": {
      "type": "date"
    },
    "checkedAt": {
      "type": "date"
    },
    "frequency": {
      "type": "number",
      "required": true
    },
    "lag": {
      "type": "number",
      "required": true
    },
    "mailNotif": {
      "type": "boolean",
      "default": false
    },
    "downtime": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "events": {
      "type": "hasMany",
      "model": "event",
      "foreignKey": "sensorId"
    },
    "outages": {
      "type": "hasMany",
      "model": "outage",
      "foreignKey": "probeId"
    }
  },
  "acls": [],
  "methods": []
}
