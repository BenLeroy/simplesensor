{
  "name": "sensor",
  "plural": "sensors",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "key": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "pattern": "/Up|Down|Flapping/",
      "required": true
    },
    "createdAt": {
      "type": "date"
    },
    "modifiedAt": {
      "type": "date"
    },
    "checkedAt": {
      "type": "date"
    },
    "frequency": {
      "type": "number",
      "required": true
    },
    "comments": {
      "type": "string"
    },
    "history": {
      "type": "array"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": []
}
